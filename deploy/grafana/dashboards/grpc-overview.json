        {"expr": "1e3 * histogram_quantile(0.99, sum(rate(budget_grpc_request_duration_seconds_bucket[5m])) by (le, method))", "legendFormat": "{{method}}", "refId": "A"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
