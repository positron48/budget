{
  "title": "Budget gRPC Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "panels": [
    {
      "type": "timeseries",
      "title": "gRPC Requests (RPS)",
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      },
      "targets": [
        {"expr": "sum(rate(budget_grpc_requests_total[1m])) by (method, code)", "legendFormat": "{{method}} {{code}}", "refId": "A"}
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "gRPC Duration p50 (ms)",
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      },
      "targets": [
        {"expr": "1e3 * histogram_quantile(0.50, sum(rate(budget_grpc_request_duration_seconds_bucket[5m])) by (le, method))", "legendFormat": "{{method}}", "refId": "A"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "type": "timeseries",
